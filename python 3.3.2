def is_perfect_square(num):
    if num < 0:
        return False
    
    sqrt = int(num ** 0.5)
    return sqrt * sqrt == num

def main():
    num = int(input("Enter any integer: "))
    
    if is_perfect_square(num):
        print("Yes")
    else:
        print("No")

if __name__ == "__main__":
    main()
