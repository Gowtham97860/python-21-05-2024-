def print_numbers_with_first_and_last_bit_set(n):
    if n <= 0:
        print("Invalid input")
        return
    
    for i in range(1, n + 1):
        # Check if the number has only the first and last bits set
        if (i & (i - 1)) == 0:
            print(i, end=" ")

def main():
    n = int(input("Enter positive number: "))
    print_numbers_with_first_and_last_bit_set(n)

if __name__ == "__main__":
    main()
