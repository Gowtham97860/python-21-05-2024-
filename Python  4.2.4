def sum_of_n_numbers(n, numbers):
    total = 0
    i = 0
    while i < n:
        total += numbers[i]
        i += 1
    return total

def main():
    n = int(input("Enter how many values do you want to read: "))
    numbers = list(map(int, input(f"Enter {n} values: ").split()))
    result = sum_of_n_numbers(n, numbers)
    print("Sum of given values:", result)

if __name__ == "__main__":
    main()
