def find_nth_square_and_cube(n):
    if n <= 0:
        print("Invalid input")
        return

    count = 0
    num = 1
    while count < n:
        if (int(num ** (1/2)) ** 2 == num) and (int(num ** (1/3)) ** 3 == num):
            count += 1
            if count == n:
                return num
        num += 1

def main():
    n = int(input("Enter a number: "))
    result = find_nth_square_and_cube(n)
    if result:
        print("Output:", result)

if __name__ == "__main__":
    main()
